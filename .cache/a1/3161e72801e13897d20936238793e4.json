display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-direction: column;\n  }\n\n  .box1 {\n    display: flex;\n    justify-content: center;\n    padding: 20px 0;\n    width: 100%;\n    background-color: #f28288;\n  }\n  \n  .box2 {\n    display: flex;\n    justify-content: center;\n    width: 100%;\n    padding: 20px 0;\n    background-color: #f8babd;\n  }\n  \n  .box3 {\n    display: flex;\n    justify-content: center;\n    width: 100%;\n    padding: 20px 0;\n    background-color: #fad1d3;\n  }\n\n  .bg {\n    border: solid transparent;\n    background: #0093a2;\n    display: flex;\n    order: 1;\n  }\n\n  .container {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    background-color: #0093a2;\n  }\n}\n\n/* Desktop Styles */\n@media only screen and (min-width: 961px) {\n  .menu {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n\n  .box1 {\n    display: flex;\n    justify-content: center;\n    padding: 20px 0;\n    width: 34%;\n    background-color: #f28288;\n  }\n  \n  .box2 {\n    display: flex;\n    justify-content: center;\n    width: 33%;\n    padding: 20px 0;\n    background-color: #f8babd;\n  }\n  \n  .box3 {\n    display: flex;\n    justify-content: center;\n    width: 34%;\n    padding: 20px 0;\n    background-color: #fad1d3;\n  }\n\n  .bg {\n    border: solid transparent;\n    background: #0093a2;\n  }\n\n  .container {\n    margin: 0 10%;\n    width: 80%;\n    max-width: 1200px;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    background-color: white;\n  }\n}\n\n/* Common Styles */\n\n.header-container {\n  color: white;\n  text-shadow: 2px 2px black;\n  background-image: url('cope.png');\n  display: flex;\n  justify-content: center;\n  border-bottom: 5px solid #f28288;\n  border-top: 5px solid #f28288;\n}\n\n.header {\n  width: 900px;\n  height: 200px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  text-align: center;\n  font-size: 50px;\n  color: white;\n  font-family: 'Josefin Sans', sans-serif;\n}\n\nbody {\n  justify-content: center;\n  color: white;\n  background: #7dc2ad;\n}\n\nh1, h2 {\n  font-family: 'Merriweather', serif;\n}\n\na {\n  font-family: 'Josefin Sans', sans-serif;\n  text-decoration: none;\n  color: black;\n}\n\n\na:hover {\n  color: #D6E9FE;\n}\n\na:visited:hover {\n  color: #D6E9FE;\n}\n\n.blog-container {\n  background: #0093a2;\n  margin-bottom: 12em;\n}\n\n.blog {\n  padding: 45px 2em;\n}\n\n.title {\n  padding: 4px 16px;\n}\n\n.date {\n  margin: 16px 32px 8px 32px;\n  font-size: 0.8em;\n}\n\n.post {\n  margin: 16px 32px 0 32px;\n  font-family: 'Open Sans', sans-serif;\n}\n\n.post ol {\n  margin: 32px 32px 32px 32px;\n  line-height: 1.75em;\n}\n\n.profilepic {\n  width: 300px;\n  float: left;\n  padding: 10px;\n}\n\n.aboutsection {\n  padding-top: 30px;\n}\n\n.about {\n  padding-left: 40px;\n  padding-top: 20px;\n  line-height: 1.25em;\n  font-family: 'Open Sans', sans-serif;\n}\n\n.emphasis {\n  color: blueviolet;\n}\n\n"},"lineCount":null}},"error":null,"hash":"202cb4e2e3744f0e6d549e27a7fd6285","cacheData":{"env":{}}}