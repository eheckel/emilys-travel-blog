<div class='container'>
  <div class='bg'>
    <div class='blog-container'>
      {{#each posts}}
          <div class='blog'>
            <h1>{{title}}</h1>
            <article>
              <p class='date'>{{postedAt}}</p>
              <p class='post'>{{{body}}}</p>
            </article>
          </div>
          {{/each}}
    </div>
  </div>
</div>